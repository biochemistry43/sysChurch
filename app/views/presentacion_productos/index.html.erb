<p id="notice"><%= notice %></p>

<h1>Listing Presentacion Productos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @presentacion_productos.each do |presentacion_producto| %>
      <tr>
        <td><%= presentacion_producto.nombre %></td>
        <td><%= link_to 'Show', presentacion_producto %></td>
        <td><%= link_to 'Edit', edit_presentacion_producto_path(presentacion_producto) %></td>
        <td><%= link_to 'Destroy', presentacion_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Presentacion producto', new_presentacion_producto_path %>

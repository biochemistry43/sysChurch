<p id="notice"><%= notice %></p>

<h1>Listing Bancos</h1>

<table>
  <thead>
    <tr>
      <th>Tipocuenta</th>
      <th>Nombrecuenta</th>
      <th>Numerocuenta</th>
      <th>Saldoinicial</th>
      <th>Fecha</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bancos.each do |banco| %>
      <tr>
        <td><%= banco.tipoCuenta %></td>
        <td><%= banco.nombreCuenta %></td>
        <td><%= banco.numeroCuenta %></td>
        <td><%= banco.saldoInicial %></td>
        <td><%= banco.fecha %></td>
        <td><%= banco.descripcion %></td>
        <td><%= link_to 'Show', banco %></td>
        <td><%= link_to 'Edit', edit_banco_path(banco) %></td>
        <td><%= link_to 'Destroy', banco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Banco', new_banco_path %>

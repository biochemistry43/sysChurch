<p id="notice"><%= notice %></p>

<h1>Listing Tipo Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombretipo</th>
      <th>Descripciontipo</th>
      <th>Isventas</th>
      <th>Isinventarios</th>
      <th>Isperdidas</th>
      <th>Ispersonas</th>
      <th>Isgastos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_usuarios.each do |tipo_usuario| %>
      <tr>
        <td><%= tipo_usuario.nombreTipo %></td>
        <td><%= tipo_usuario.descripcionTipo %></td>
        <td><%= tipo_usuario.isVentas %></td>
        <td><%= tipo_usuario.isInventarios %></td>
        <td><%= tipo_usuario.isPerdidas %></td>
        <td><%= tipo_usuario.isPersonas %></td>
        <td><%= tipo_usuario.isGastos %></td>
        <td><%= link_to 'Show', tipo_usuario %></td>
        <td><%= link_to 'Edit', edit_tipo_usuario_path(tipo_usuario) %></td>
        <td><%= link_to 'Destroy', tipo_usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo usuario', new_tipo_usuario_path %>

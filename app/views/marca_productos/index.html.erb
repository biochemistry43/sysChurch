<p id="notice"><%= notice %></p>

<h1>Listing Marca Productos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @marca_productos.each do |marca_producto| %>
      <tr>
        <td><%= marca_producto.nombre %></td>
        <td><%= link_to 'Show', marca_producto %></td>
        <td><%= link_to 'Edit', edit_marca_producto_path(marca_producto) %></td>
        <td><%= link_to 'Destroy', marca_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Marca producto', new_marca_producto_path %>
